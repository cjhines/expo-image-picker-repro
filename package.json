{
  "name": "MyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "android:device": "react-native run-android -- --device",
    "ios:device": "react-native run-ios --device",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "ts-check": "tsc",
    "format": "prettier --write \"**/*.{ts,tsx,js,json}\"",
    "postinstall": "cd ios && pod install",
    "extract-loc": "formatjs extract 'src/**/!(*.d).{ts,tsx,js,jsx}' --out-file './src/intl/translations/en.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile-loc": "formatjs compile-folder ./src/intl/translations ./src/intl/locales",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-navigation/material-top-tabs": "^6.6.4",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/stack": "^6.3.18",
    "@reduxjs/toolkit": "^1.9.6",
    "react": "18.2.0",
    "react-intl": "^6.4.7",
    "react-native": "0.72.4",
    "react-native-gesture-handler": "^2.13.1",
    "react-native-mmkv": "^2.10.1",
    "react-native-pager-view": "^6.2.1",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-tab-view": "^3.5.2",
    "react-redux": "^8.1.2",
    "redux-persist": "^6.0.0",
    "expo": "^49.0.0",
    "expo-image-picker": "~14.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@formatjs/cli": "^6.2.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-formatjs": "^10.3.35",
    "eslint": "^8.19.0",
    "eslint-config-universe": "^11.3.0",
    "eslint-plugin-formatjs": "^4.3.9",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^14.0.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-native-flipper": "^0.182.0",
    "react-native-mmkv-flipper-plugin": "^1.0.0",
    "react-test-renderer": "18.2.0",
    "redux-flipper": "^2.0.2",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
